tails({...postDetails, post_description: e.target.value})} />\n\n                <div>\n                    <input \n                        type=\"checkbox\" \n                        id=\"html\" \n                        onChange={(e) => handleTags(e)}\n                        value=\"html\" \n                    />\n                    <label>HTML</label>\n                </div>\n                <div>\n                    <input \n                        type=\"checkbox\" \n                        id=\"javascript\" \n                        onChange={(e) => handleTags(e)}\n                        value=\"javascript\" \n                    />\n                    <label>Javascript</label>\n                </div>\n                <div>\n                    <input \n                        type=\"checkbox\" \n                        id=\"css\" \n                        onChange={(e) => handleTags(e)}\n                        value=\"css\" \n                    />\n                    <label>css</label>\n                </div>\n                <input type=\"submit\" value=\"submit\" />\n            </form>\n        </div>\n    )\n}\n\nexport default PostModal\n",["87","88"],"/home/joseph-main/websites/webdev-news/webdev-news-react/src/components/UserModal.jsx",["89","90","91","92"],"/home/joseph-main/websites/webdev-news/webdev-news-react/src/components/Message.jsx",[],"/home/joseph-main/websites/webdev-news/webdev-news-react/src/components/Footer.jsx",[],"/home/joseph-main/websites/webdev-news/webdev-news-react/src/messageContext.js",[],"/home/joseph-main/websites/webdev-news/webdev-news-react/src/webContext.js",[],"/home/joseph-main/websites/webdev-news/webdev-news-react/src/reportWebVitals.js",[],"/home/joseph-main/websites/webdev-news/webdev-news-react/src/userContext.js",[],{"ruleId":"93","severity":1,"message":"94","line":16,"column":44,"nodeType":"95","messageId":"96","endLine":16,"endColumn":45},{"ruleId":"97","severity":1,"message":"98","line":62,"column":17